
class Deck{
	private int numItems;
	
	public Deck(){
		List D = new List();
		for (int i = 0; i < 52; i++){
			String s;
			String r;
			switch (i%13){
				case (0):
					s = i;
					r = "";
					break;
				case (1):
					s = i;
					r = "";
					break;
				case (2):
					s = i;
					r = "";
			}
			Card c = new Card();
			D.append(c);
		}
		numItems = D.length();
	}
	
	public void shuffle(){
		
	}
	
	
	
	private class Card{
		private String suit;
		private String rank;
		
		private Node(String s, String r){
			suit = s;
			rank = r;
		}
		
		private String getSuit(){
			return suit;
		}
		
		private String getRank(){
			return rank;
		}
		
		private void setSuit(String newSuit){
			suit = newSuit;
		}
		
		private void setRank(String newRank){
			rank = newRank;
		}
		
		public boolean equals(Object x){
			boolean equal = false;
			if (x instanceof Card){
				Card N = (Card)x;
				if (rank.equals(N.rank) && suit.equals(N.suit)){
					equal = true;
				}
			}
			return equal;
		}
		
		public String toString(){
			String str = rank+" of "+suit;
			return str;
		}
	}
}