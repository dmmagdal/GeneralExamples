
public class Deck{
	private Node front;
	private Node back;
	private Node cursor;
	private int index;
	private int numItems;
	
	public Deck(){
		front = null;
		back = front;
		cursor = back;
	}
	
	private class Node{
		private String suit;
		private String rank;
		
		public Node(String s, String r){
			suit = s;
			rank = r;
		}
		
		private String getSuit(){
			return suit;
		}
		
		private String getRank(){
			return rank;
		}
		
		private void setSuit(String newSuit){
			suit = newSuit;
		}
		
		private void setRank(String newRank){
			rank = newRank;
		}
	}
}