
public class Deck{
	private Node front;
	private Node back;
	private Node cursor;
	private int index;
	private int numItems;
	
	public Deck(){
		front = null;
		back = front;
		cursor = back;
		index = -1;
		numItems = 0;
	}
	
	
	
	private class Node{
		private String suit;
		private String rank;
		private Node next;
		private Node prev;
		
		public Node(String s, String r){
			suit = s;
			rank = r;
			next = null;
			prev = null;
		}
		
		private String getSuit(){
			return suit;
		}
		
		private String getRank(){
			return rank;
		}
		
		private Node getNext(){
			return next;
		}
		
		private Node getPrev(){
			return prev;
		}
		
		private void setSuit(String newSuit){
			suit = newSuit;
		}
		
		private void setRank(String newRank){
			rank = newRank;
		}
		
		private void setNext(Node nx){
			next = nx;
		}
		
		private void setPrev(Node pv){
			prev = pv;
		}
		
		private boolean equals(Object x){
			boolean equal = false;
			if (x instanceof Node){
				Node N = (Node)x;
				if (rank.equals(N.rank) && suit.equals(N.suit)){
					boolean = true;
				}
			}
			return equal;
		}
		
		private String toString(){
			
		}
	}
}