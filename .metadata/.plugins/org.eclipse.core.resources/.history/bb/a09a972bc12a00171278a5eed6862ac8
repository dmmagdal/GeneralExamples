
public class NumList {
	private static Node head;
	private int numitems;
	
	public NumList(){
		head = new Node();
		numitems = 0;
	}
	
	public void insert(int n, int x, int y){
		Node N = new Node(n, x, y);
		if (head.num == 0){
			head = N;
		}
		else{
			N.next = head;
			head = N;
		}
		numitems++;
	}
}
